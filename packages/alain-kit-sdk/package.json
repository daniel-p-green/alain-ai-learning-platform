{
  "name": "alain-kit-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "ALAIN-Kit SDK: outline-first notebook generation with quality validation and Colab compatibility.",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bin/alain-kit": {
      "import": "./dist/bin/alain-kit.js"
    }
  },
  "bin": {
    "alain-kit": "dist/bin/alain-kit.js"
  },
  "dependencies": {
    "alain-kit": "file:../alain-kit"
  },
  "scripts": {
    "example": "tsx examples/usage-example.ts",
    "cli": "tsx bin/alain-kit.ts",
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
